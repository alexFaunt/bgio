{
  "name": "bgio-poker",
  "version": "0.0.3",
  "description": "hello there",
  "main": "build/server/index.js",
  "scripts": {
    "start:dev": "concurrently \"yarn start:client:dev\" \"docker-compose up\" \"WATCH=true yarn generate:types\"",
    "start:client:dev": "webpack-dev-server",
    "start:server:dev": "babel-watch --extensions '.js,.json,.ts' ./src/server/index.ts",
    "build:client": "webpack",
    "lint": "eslint src",
    "tsc": "tsc --noImplicitAny",
    "migrate": "ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/.bin/knex --knexfile ./src/server/db/knexfile.ts migrate:latest;",
    "rollback": "ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/.bin/knex --knexfile ./src/server/db/knexfile.ts migrate:rollback;",
    "generate:migration": "hygen migration create",
    "generate:types": "babel-node --extensions '.js,.json,.ts' ./scripts/generate-types.ts"
  },
  "author": "",
  "license": "ISC",
